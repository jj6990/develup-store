{
  "kind": "collectionType",
  "collectionName": "houses",
  "info": {
    "singularName": "house",
    "pluralName": "houses",
    "displayName": "House"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "location": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "gallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "mainImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "house"
    }
  }
}
