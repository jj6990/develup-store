{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderId": {
      "type": "uid"
    },
    "userId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "orderDate": {
      "type": "datetime"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "processing",
        "shipped",
        "delivered",
        "cancelled",
        "on hold",
        "backordered",
        "refunded",
        "pending payment",
        "partially shipped"
      ]
    },
    "billingAddress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::billing-address.billing-address"
    },
    "shippingAddress": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shipping-address.shipping-address"
    },
    "shipping_method": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shipping-method.shipping-method"
    },
    "payment_method": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment-method.payment-method"
    }
  }
}
